{
  "name": "react-native-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "run:android": "react-native run-android",
    "run:ios": "react-native run-ios",
    "run:start": "react-native start",
    "run:storybook": "start-storybook",
    "build:storybook": "build-storybook",
    "test:run": "jest",
    "lint:run": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "prerun:storybook": "rnstl"
  },
  "dependencies": {
    "@react-native-community/cameraroll": "^4.0.1",
    "@react-native-community/segmented-control": "^2.2.2",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/viewpager": "^4.2.2",
    "@react-native-picker/picker": "^1.9.10",
    "@types/react-native-drawer": "^2.5.3",
    "@types/react-native-i18n": "^2.0.0",
    "@types/react-native-side-menu": "^1.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-native": "^5.1.0",
    "@types/redux": "^3.6.0",
    "axios": "^0.21.1",
    "lottie-react-native": "^3.5.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-eject": "^0.1.2",
    "react-native-elements": "^3.1.0",
    "react-native-i18n": "^2.0.15",
    "react-native-safe-area-context": "^3.1.9",
